{
  "name": "rest-countries",
  "version": "0.1.0",
  "description": "A task manager built with Next.js React Framework, Tailwind CSS, Framer Motion",
  "private": true,
  "repository": "git@github.com:princemuel/rest-countries.git",
  "author": "princemuel <vansomecsam@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "rm -rvf .next && next dev",
    "build": "next build",
    "start": "next start",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "type-check": "tsc --noEmit",
    "format:write": "prettier --write \"{app,lib,components}/**/*.{ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"{app,lib,components}**/*.{ts,tsx,mdx}\" --cache"
  },
  "engines": {
    "node": "18.x",
    "npm": "please-use-pnpm",
    "yarn": "please-use-pnpm",
    "pnpm": "8.x"
  },
  "dependencies": {
    "@plaiceholder/next": "^3.0.0",
    "@types/node": "20.4.10",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "10.4.14",
    "cva": "npm:class-variance-authority@^0.7.0",
    "eslint": "8.47.0",
    "eslint-config-next": "13.4.13",
    "lucide-react": "0.263.1",
    "next": "13.4.13",
    "next-themes": "^0.2.1",
    "plaiceholder": "^3.0.0",
    "postcss": "8.4.27",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sharp": "^0.32.4",
    "tailwindcss": "3.3.3",
    "typescript": "5.1.6"
  },
  "devDependencies": {
    "prettier": "^3.0.1",
    "prettier-plugin-tailwindcss": "^0.5.2",
    "tailwind-merge": "^1.14.0",
    "tailwind-scrollbar": "^3.0.5"
  }
}
